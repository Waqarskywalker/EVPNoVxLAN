forwarding-options {
    storm-control-profiles default {
        all;
    }
}
routing-options {
    router-id 3.3.3.3;
    autonomous-system 65002;
    forwarding-table {
        export load-balance;
    }
}
protocols {
    bgp {
        group underlay {
            type external;
            family inet {
                unicast;
            }
            export lo0;
            multipath multiple-as;
            neighbor 1.0.0.0 {
                peer-as 65000;
            }
            neighbor 1.0.0.4 {
                peer-as 65001;
            }
        }
        group EVPN_VxLAN {
            type external;
            multihop {
                ttl 255;
                no-nexthop-change;
            }
            local-address 3.3.3.3;
            family evpn {
                signaling;
            }
            neighbor 1.1.1.1 {
                peer-as 65000;
            }
            neighbor 2.2.2.2 {
                peer-as 65001;
            }
            neighbor 4.4.4.4 {
                peer-as 65003;
            }
        }
    }
    evpn {
        encapsulation vxlan;
        extended-vni-list [ 1100 1200 1300 1400 ];
        multicast-mode ingress-replication;
        vni-options {
            vni 1100 {
                vrf-target export target:1:100;
            }
            vni 1200 {
                vrf-target export target:1:200;
            }
            vni 1300 {
                vrf-target export target:1:300;
            }
            vni 1400 {
                vrf-target export target:1:400;
            }
        }
    }
    igmp-snooping {
        vlan default;
    }
}
policy-options {
    policy-statement lo0 {
        term 1 {
            from {
                route-filter 3.3.3.3/32 exact;
            }
            then accept;
        }
    }
    policy-statement load-balance {
        term 1 {
            then {
                load-balance per-packet;
            }
        }
    }
    policy-statement vrf-imp {
        term t1 {
            from community com100;
            then accept;
        }
        term t2 {
            from community com200;
            then accept;
        }
        term t3 {
            from community com300;
            then accept;
        }
        term t4 {
            from community com400;
            then accept;
        }
    }
    community com100 members target:1:100;
    community com200 members target:1:200;
    community com300 members target:1:300;
    community com400 members target:1:400;
}
switch-options {
    vtep-source-interface lo0.0;
    route-distinguisher 3.3.3.3:1;
    vrf-import vrf-imp;
    vrf-target target:9999:9999;
}
vlans {
    default {
        vlan-id 1;
    }
    v100 {
